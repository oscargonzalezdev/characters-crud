<h1>List of characters from the API:</h1>

{{#each characters}}
    <section class="character-summary">
        <h2>{{this.name}}</h2>
        <p>
            <a href="/characters/{{this.id}}">More details</a> |
            <a href="/characters/{{this.id}}/edit">Edit</a>
        </p>

    </section>
{{/each}}


